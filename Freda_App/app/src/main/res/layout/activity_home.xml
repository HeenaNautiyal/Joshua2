<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background"
    android:orientation="vertical"
    android:weightSum="5"
  >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="102dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:orientation="horizontal"
        android:weightSum="2"
        android:layout_weight="1"
        android:id="@+id/lay1"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_marginTop="8dp">
    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:id="@+id/innerlay1">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="6dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/home_2"
            android:id="@+id/home"
            />
    </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/innerlay2">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="6dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/frames_2"
                android:id="@+id/collage"
                />
        </LinearLayout>

</LinearLayout>


  <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="102dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:orientation="horizontal"
        android:weightSum="2"
        android:id="@+id/lay2"
        android:layout_weight="1"
        android:layout_below="@+id/lay1"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_marginTop="4dp">
    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1">

    <ImageView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:paddingTop="6dp"
       android:layout_marginTop="16dp"
       android:src="@drawable/custom_frames_2"
       android:id="@+id/customframe"
       />
</LinearLayout>
<LinearLayout
   android:layout_width="0dp"
   android:layout_height="fill_parent"
   android:layout_weight="1">

   <ImageView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:paddingTop="6dp"
       android:layout_marginTop="16dp"
       android:src="@drawable/blogs_2"
       android:id="@+id/blog"
       />
</LinearLayout>

</LinearLayout>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="102dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:orientation="horizontal"
        android:weightSum="2"
        android:layout_weight="1"
        android:id="@+id/lay3"
        android:layout_below="@+id/lay2"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_marginTop="4dp">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="6dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/gift_2"
                android:id="@+id/gift"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="6dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/account_2"
                android:id="@+id/account"
                />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="96dp"
        android:orientation="horizontal"
        android:weightSum="1"
        android:layout_weight="1"
        android:layout_gravity="center"
        android:layout_marginTop="4dp"
        android:id="@+id/lay4"
        android:layout_below="@+id/lay3"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:paddingLeft="14dp"
        android:paddingRight="14dp"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center"
            >
            <ImageView
                android:id="@+id/i_contact"
                android:layout_width="wrap_content"
                android:paddingTop="6dp"
                android:layout_marginTop="2dp"
                android:layout_height="wrap_content"
                android:src="@drawable/contact_2" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:weightSum="3"
        android:layout_gravity="center"
         android:id="@+id/lay5"
        android:layout_below="@+id/lay4"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:gravity="center"
            >
            <ImageView
                android:id="@+id/facebookshare"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/ic_facebooknew"
                android:layout_below="@+id/lay4"
                android:layout_centerHorizontal="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginTop="8dp"
            android:gravity="center"
            >
            <ImageView
                android:id="@+id/emailshare"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/ic_mail"
                android:layout_below="@+id/lay4"
                android:layout_centerHorizontal="true" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:gravity="center"
            >
            <ImageView
                android:id="@+id/twittershare"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/ic_twitternew"
                android:layout_below="@+id/lay4"
                android:layout_centerHorizontal="true" />
        </LinearLayout>
    </LinearLayout>



</LinearLayout>
